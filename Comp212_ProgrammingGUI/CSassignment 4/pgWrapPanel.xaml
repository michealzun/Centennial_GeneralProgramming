<Page x:Class="aaaaaa.pgWrapPanel"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:aaaaaa"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="pgWrapPanel">

    <WrapPanel Margin="0,0,-7,0">
        <Label Content="1" Width="90" Background="AliceBlue"/>
        <Label Content="2" Width="90" Background="Salmon"/>
        <Label Content="3" Width="90" Background="DarkCyan"/>
        <Label Content="4" Width="90" Background="HotPink"/>
        <Label Content="5" Width="90" Background="OldLace"/>
        <Label Content="6" Width="90" Background="Yellow"/>
    </WrapPanel>
</Page>
